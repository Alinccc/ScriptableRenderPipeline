VisualElement > #VectorControl {
    flex-direction: row;
    flex-grow: 1;
    align-items: center;
    margin-top: 2;
    margin-bottom: 2;
}

#dummy > Label {
    margin-left: 0;
    margin-right: 0;
    margin-bottom: 0;
    cursor: slide-arrow;
    text-alignment: middle-left;
}

.unity-base-field {
    flex: 1;
    margin-top: 0;
    margin-bottom: 0;
    text-alignment: middle-left;
}

unity-base-field__input{
    margin-left: 0;
    margin-right: 0;
}

FloatField.unity-double-field {
    padding-left: 0;
    padding-right: 0;
}
